{
  "manifest_version": 3,
  "name": "TitlePeek",
  "version": "1.0",
  "description": "Fetches and presents the current page title in a clean popup.",
  "permissions": ["tabs"],
  
  "action": {
    "default_popup": "popup.html",

    "default_icon":{
          "48": "img/icon_48x48.png"
    }
 },
    "icons": {
      "128": "img/icon_128x128.png"
} 
 
}
